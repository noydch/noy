{
    "rewrites": [
        { "source": "/(.*)", "destination": "/api/index.ts" }
    ],
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18.x"
      }
    },
    "build": {
      "command": "npm run build",
      "output": "dist"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }